{
  "name": "hosts-monitor",
  "version": "0.1.0",
  "binary": "../symbion-plugin-hosts/target/debug/symbion-plugin-hosts",
  "description": "Monitoring des hosts avec heartbeats et Wake-on-LAN",
  "contracts": ["heartbeat@v2", "wake@v1"],
  "auto_start": false,
  "restart_on_failure": true,
  "startup_timeout_seconds": 15,
  "shutdown_timeout_seconds": 5,
  "depends_on": [],
  "start_priority": 10,
  "env": {
    "SYMBION_WOL_BACKENDS": "udp",
    "SYMBION_WOL_UDP_PORTS": "9,7"
  }
}